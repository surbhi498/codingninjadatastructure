// arr - input array
// size - size of array

int DuplicateNumber(int arr[], int size){
    /* Don't write main().
     * Don't read input, it is passed as function argument.
     * Return output and don't print it.
     * Taking input and printing output is handled automatically.
 
 
 */
int t = size-2;
int count[t] = {0};    
for(int j = 0;j<size;j++){
    
count[(arr[j])]++;   
    
    
    
    
    
}    
    
    
 for(int g = 0;g<t;g++){
     
 if(count[g] == 2){
     
    
return g;    
     
     
     
 }    
     
    
    

}
    
    
}    
    
