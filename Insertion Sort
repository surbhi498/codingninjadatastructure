// arr - input array
// n - size of array
void InsertionSort(int arr[], int n){
	// Write your code here
int i =0;    
while(i<n){
int j = i-1; 
int temp = arr[i];    
while(j>=0 && temp<arr[j]){
    
//int temp = arr[j];  
arr[j+1]=arr[j];    
j--;    
    
}    
    
//arr[j+1]=arr[j];
arr[j+1]= temp;
    
i++;    
    
    
    
}
    
    
    
    
    
}
