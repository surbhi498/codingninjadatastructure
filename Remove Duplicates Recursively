
#include <iostream> 
#include <string.h>
#include<stdio.h>
using namespace std; 

void removeConsecutiveDuplicates(char *input) {
	/* Don't write main().
	* Don't read input, it is passed as function argument.    
	* Change in the given string itself.
	* No need to return or print anything
	* Taking input and printing output is handled automatically.
	*/

int n = strlen(input);
if(n<1){
    
return;    
    
    
}    
else if(n==1){
    
//do nothing;    
    
    
}    
else if(n==2){
    
if(input[0]==input[1]){
int h;
for( h=1;h<2;h++){
    
input[h-1]=input[h];    
    
}    
input[h-1]='\0';    
    
    
    
}    
else{
    
//do nothing;    
    
    
    
}    
    
    
    
}    
    
    
else
{
    
removeConsecutiveDuplicates(input+1);
int n1 = strlen(input);    
if(input[0]==input[1]){
int h;    
for(h =1;h<n1;h++)
{
    
input[h-1]=input[h];    
    
    
}    
input[h-1]='\0';    
    
    
}    
else{
    
//removeConsecutiveDuplicates(input+1);    
    
    
}    
    
    
}    
    
}    
    
    




