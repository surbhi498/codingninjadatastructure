// Change in the given string itself. So no need to return or print anything
#include <bits/stdc++.h>
#include <iostream> 
#include <string.h> 
#include<stdio.h>
using namespace std; 
void removeX(char input[]) {
    // Write your code here

int n = strlen(input);
if(n<1)
return;
else if(n==1){
if(input[0]== 'x'){
    
input[0]= '\0';    
    
    
}    
else{
    
//do nothing;    
    
    
    
}    
    
    
}    
else{
    
    
removeX(input+1);    
    
    
    
   
    
if(input[0]=='x'){
    
int k = (strlen(input));
int g = 1;    
while(g<k){
    
input[g-1]=input[g];    
    
g++;    
    
}    
input[g-1]='\0';    
    
    
}    
else{
    
    
//do nothing;    
    
    
}    
    
}    
    
}
