#include<iostream>
using namespace std;

int main(){
    /* Read input as specified in the question.
     * Print output as specified in the question.
 
 
 */
    
int n;
cin>>n;    
int a[n]; 
int sum = 0;   
for(int i = 0;i<n;i++){
cin>>a[i];    
    
    
} 
    
 for(int j = 0;j<n;j++){
     
sum += a[j];     
     
     
 }   
cout<<sum;    
    
}


