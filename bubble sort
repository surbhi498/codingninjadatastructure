// arr - input array
// n - size of array
void BubbleSort(int arr[], int n){
	// Write your code here

for(int i =0;i<(n);i++){
    
for(int j = 0;j<(n-i-1);j++){
int temp;   
if(arr[j]>arr[j+1])
{
    
temp = arr[j];
arr[j] = arr[j+1];
arr[j+1] = temp;    
    
    
    
    
}    
    
}    
    
    
    
    
}    
    
    
    
}
